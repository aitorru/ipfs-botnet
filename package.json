{
  "name": "ipfs-botnet",
  "version": "1.0.0",
  "description": "A botnet implementation over ipfs pubsub.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"npm:watch:build\"  \"npm:watch:dev\" ",
