{
  "name": "ipfs-botnet",
  "version": "1.0.0",
  "description": "A botnet implementation over ipfs pubsub.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"npm:watch:build\"  \"npm:watch:dev\" ",
    "build": "tsc",
    "watch:build": "tsc -w",
    "watch:dev": "nodemon dist/index.js",
    "lint": "eslint --fix .",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'src/test/**/*.ts'"
  },
  "repository": "https://github.com/aitorru/ipfs-botnet.git",
  "author": "Aitor Ruiz <aitorruiz2m@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/args": "^5.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "chai": "^4.3.6",
    "concurrently": "^7.1.0",
    "eslint": "^8.13.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "args": "^5.0.1",
    "axios": "^0.27.2",
    "ipfs-core": "^0.14.2",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1"
  }
}
