  "author": "Aitor Ruiz <aitorruiz2m@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/args": "^5.0.0",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.13.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "args": "^5.0.1"
  }
}